<Window x:Class="ScientistsApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScientistsApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Style="{StaticResource GridUp}">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>

        <TextBlock Text="Практические работы по Entity Framework. Выполнил: студент гр. ИСП.21.1А Самозванцев Владимир. Вариант: 18."
                   Style="{StaticResource TBlockUp}"/>
        <Frame x:Name="_mainFrame" NavigationUIVisibility="Hidden"
               Grid.Row="2"/>
    </Grid>
</Window>
